<div id="create">
	<div class="table-responsive overflow-y:auto", style="max-height:360px;">
		<table border="2" class="table table-striped table-bordered table-hover">
			<thead>
				<tr>
					<th>Question</th>
					<th>A</th>
					<th>B</th>
					<th> C</th>
					<th>D</th>
					<th>Answer</th>
				</tr>
			</thead>
			<tbody>
				<% @questions.order('id DESC').each do |question| %>
				<tr>
					<td>
						<%=question.question %>
					</td>
					<td>
						<%=question.option1 %>
					</td>
					<td>
						<%=question.option2 %>
					</td>
					<td>
						<%=question.option3 %>
					</td>
					<td>
						<%=question.option4 %>
					</td>
					<td>
						<%=question.answer %>
					</td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>
<%= disable_add_question_button %>